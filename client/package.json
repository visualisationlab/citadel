    "react-chartjs-2": "^4.3.1",
    "react-dev-tools": "^0.0.1",
    "react-dom": "^18",
    "react-icons": "^4.4.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
