    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.8.2",
    "cytoscape": "^3.21.1",
    "d3": "^7.6.1",
    "date-fns": "^2.28.0",
    "fuse.js": "^6.6.2",
    "http-proxy-middleware": "^2.0.4",
    "immutable": "^4.2.3",
    "jsonschema": "^1.4.1",
    "mathjs": "^10.6.4",
    "msgpackr": "^1.6.2",
    "node": "^19.6.0",
    "object-hash": "^3.0.0",
    "pixi.js": "^5.3.12",
    "qrcode": "^1.5.1",
    "queue": "^6.0.2",
    "react": "^18",
    "react-bootstrap": "^2.4.0",
    "react-chartjs-2": "^4.3.1",
    "react-dev-tools": "^0.0.1",
    "react-dom": "^18",
    "react-icons": "^4.4.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-validation": "^3.0.7",
    "sass": "^1.59.3",
    "tinycolor": "^0.0.1",
    "tinycolor2": "^1.4.2",
    "typescript": "^4.6.4",
    "validator": "^13.7.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "server": "npx tsc --build; PORT=8060 react-scripts start",
    "localdev": "npx tsc; HTTPS=true PORT=3000; react-scripts start",
    "local": "docker run -u=1000:1000 -v $(pwd):/app -w=/app -d -p 3000:3000 --rm --name=nodedev node bash -c \"npm install && npm run dev\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prod": "serve -s build --ssl-cert \"../certs/dev.visgraph.pem\" --ssl-key \"../certs/dev.visgraph-key.pem\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@types/cytoscape": "^3.19.4",
    "@types/d3": "^7.1.0",
    "@types/deep-equal": "^1.0.1",
    "@types/node": "^17.0.33",
    "@types/object-hash": "^2.2.1",
    "@types/qrcode": "^1.5.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/tinycolor2": "^1.4.3",
    "babel-loader": "^9.1.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "webpack": "^5.75.0"
  },
  "overrides": {
    "react-refresh": "0.11.0"
  }
}
