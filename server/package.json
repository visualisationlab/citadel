{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "main": "app.js",
  "scripts": {
    "start": "mkdir logs; npm run compile && node dist/src/app.js",
    "compile": "mkdir lib; mkdir types; mkdir types/generated; npm run clean && npx tsc",
    "clean": "rm -r types/generated/*; rm tsconfig.tsbuildinfo; find ./src/ -type f -name '*.d.ts' -delete && find ./src/ -type f -name '*.js' -delete && find ./src/ -type f -name '*.jsx' -delete && rm -rf ./lib/ && rm -rf ./build/"
  },
